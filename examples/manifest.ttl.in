@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix lvtkp: <http://lvtoolkit.org/plugins/> .
@prefix ui:    <http://lv2plug.in/ns/extensions/ui#> .
@prefix kv:    <https://kushview.net/lv2#> .

<http://lvtoolkit.org/people/mfisher>
	a foaf:Person ;
	foaf:name "Michael Fisher" ;
	foaf:mbox <mailto:mfisher31@gmail.com> ;
	rdfs:seeAlso <http://lvtoolkit.org> .

lvtkp:
	a doap:Project ;
	lv2:symbol "lvtk" ;
	doap:name "LVTK Examples" ;
	doap:shortdesc "Example plugins demonstrating the LVTK api." ;
	doap:homepage <http://lvtoolkit.org> ;
	doap:maintainer <http://lvtoolkit.org/people/mfisher> ;
	doap:developer <http://lvtoolkit.org/people/mfisher> .

lvtkp:silence
	a lv2:Plugin ;
	lv2:binary <silence@LIB_EXT@> ;
	rdfs:seeAlso <silence.ttl> .

<http://lvtoolkit.org/plugins/silence#ui>
	a ui:GtkUI ;
	ui:binary <silence_ui@LIB_EXT@> ;
	rdfs:seeAlso <silence.ttl> .

lvtkp:workhorse
	a lv2:Plugin ;
	lv2:binary <workhorse@LIB_EXT@> ;
	rdfs:seeAlso <workhorse.ttl> .

<http://lvtoolkit.org/plugins/workhorse#ui>
	a ui:GtkUI ;
	ui:binary <workhorse_ui@LIB_EXT@> ;
	rdfs:seeAlso <workhorse.ttl> .

<http://ll-plugins.nongnu.org/lv2/lv2pftci/beep>
	a lv2:Plugin ;
	lv2:binary <beep@LIB_EXT@> ;
	rdfs:seeAlso <beep.ttl> .

<http://ll-plugins.nongnu.org/lv2/lv2pftci/beep/gui>
	a ui:GtkUI;
	ui:binary <beep_ui@LIB_EXT@> ;
	rdfs:seeAlso <beep.ttl> .

lvtkp:volume
	a lv2:Plugin ;
	lv2:binary <volume@LIB_EXT@> ;
	rdfs:seeAlso <volume.ttl> .

<http://lvtoolkit.org/plugins/volume#ui>
	a kv:JUCEUI ;
	ui:binary <volume_ui@LIB_EXT@> ;
	rdfs:seeAlso <volume.ttl> .
